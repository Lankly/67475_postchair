<p id="notice"><%= notice %></p>

<h1>Listing Input Streams</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Position</th>
      <th>Input time</th>
      <th>Measurement</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @good_result.each do |key,value| %>
    <tr>
        <td><%= key %></td>
        <td><%= value %></td>
    </tr>
    <% end %>
    <% @input_streams.each do |set,input| %>
        <% input.each do |input_stream| %>
      <tr>
        <td><%= input_stream.set_id %></td>
        <td><%= input_stream.position %></td>
        <td><%= input_stream.created_at.strftime("Printed on %m/%d/%I:%M") %></td>
        <td><%= input_stream.measurement %></td>
        <td><%= link_to 'Show', input_stream %></td>
        <td><%= link_to 'Edit', edit_input_stream_path(input_stream) %></td>
        <td><%= link_to 'Destroy', input_stream, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Input stream', new_input_stream_path %>
